				MX6ULL_PAD_SNVS_TAMPER5__GPIO5_IO05		0x000110A0
			>;
		};

		pinctrl_sii902x: hdmigrp {
			fsl,pins = <
				MX6UL_PAD_UART1_CTS_B__GPIO1_IO18 0x59
